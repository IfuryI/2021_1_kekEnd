{
    "env": {
        "browser": true,
        "node": true,
        "es2021": true
    },
    "extends": [
      "eslint:recommended",
      "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
      "indent": ["error", 4],
      "max-len": ["error", 120, 2, {
        "ignoreUrls": true,
        "ignoreComments": true,
        "ignoreRegExpLiterals": true,
        "ignoreStrings": false,
        "ignoreTemplateLiterals": false
      }],
      "require-jsdoc": "off" // убрать после JSDoc
    }
}
